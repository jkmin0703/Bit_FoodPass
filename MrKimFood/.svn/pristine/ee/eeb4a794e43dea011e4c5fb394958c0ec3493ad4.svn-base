<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="orderDetail">
	
    <resultMap id="orderDetailMap" type="com.mrkimfood.gonggam.vo.OrderDetailVO">
    	<result column="order_id" property="orderId"/>
    	<result column="no" property="tableNo"/>
    	<result column="order_day" property="orderDay"/>
    	<result column="order_time" property="orderTime"/>
    	<result column="restaurant_name" property="restaurantName"/>
    	<result column="food_name" property="foodName"/>
    	<result column="pay" property="pay"/>
    	<result column="count" property="orderQuantity"/>
    	<result column="order_sum" property="orderSum"/>
    	<result column="is_supply" property="isSupply"/>
    </resultMap>
    
</mapper>